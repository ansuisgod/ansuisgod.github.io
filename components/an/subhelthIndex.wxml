<!-- 亚健康视图模板 -->
<template name="subhelthIndex">
<!-- 肥胖视图模板 -->
  <view class='obesityBox' wx:for-items="{{obesity}}" wx:key='property' bindtap='goDetailInfo' data-articlid='{{item.id}}'>
        <view class='topBox'>
          <view class='leftBox'>
            <image wx:if='{{item.user.head_icon}}' src='{{item.user.head_icon}}'></image>
            <image wx:else src='https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1561638291636&di=bae0f8e67e988b9522d8c6010f54c599&imgtype=0&src=http%3A%2F%2Fhbimg.b0.upaiyun.com%2Fa12f24e688c1cda3ff4cc453f3486a88adaf08cc2cdb-tQvJqX_fw658'></image>
          </view>
          <view class='rightBox'>{{item.user.nick_name}}</view>
        </view>
        <view class='centerBox'>
          <view class='centerLeftBox'>
            <view class='titleBox'>{{item.title}}</view>
            <view class='mainBox'>{{item.summary.text}}</view>
          </view>
          <view class='centerRightBox'>
            <image src='{{item.summary.images[0].url}}'></image>
          </view>
        </view>
        <view class='bottomBox'>
          <!-- 浏览粗疏 -->
          <view class='leftB'>
            <view class='leftBoxs'>
              <image src='../../static/eye.png'></image>
            </view>
            <view class='look'>{{item.view_size}}</view>
          </view>
          <!-- 点赞的次数 -->
          <view class='rightB'>
            <view class='boxRight'>
              <image src='../../static/like.png'></image>
            </view>
            <view class='like'>{{item.like_num}}</view>
          </view>
        </view>
      </view>
  
